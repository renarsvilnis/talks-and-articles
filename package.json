{
  "private": true,
  "name": "talks-and-articles",
  "version": "1.0.0",
  "description": "List of all tech related articles, talks, tutorials etc. that I have seen and read.",
  "bin": {
    "taa": "./bin/taa.js"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "push": "git pull && git add -A && git commit -m 'New items' && git push",
    "lint": "eslint --fix ./lib",
    "add": "./bin/taa.js taa-add",
    "build:dev": "NODE_ENV=development node ./webpackDevServer.js",
    "build:prod": "NODE_ENV=production webpack --progress --config=./webpack.config.production",
    "build:clean": "rm -rf ./dist/*",
    "build:profile": "yarn run build:prod --profile --json > ./webpack.stats.json",
    "build:remove-sourcemaps": "rm ./dist/*.map",
    "dev": "yarn run build:dev",
    "prod": "yarn run build:clean && yarn run build:prod",
    "local-server": "http-server -p 8080 ./dist",
    "demo": "yarn run prod && node demoServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renarsvilnis/talks-and-articles.git"
